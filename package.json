{
  "name": "ngx-daterangepicker-material",
  "version": "6.0.4",
  "description": "Angular 17 datepicker",
  "keywords": [
    "angular17",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --configuration production --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p ng-package.json -c tsconfig.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && shx cp ./schematics/collection.json ./dist/schematics && shx cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "peerDependencies": {
    "dayjs": "^1.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "^17.5.2",
    "@angular-eslint/template-parser": "^17.5.2",
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^16.2.4",
    "@angular/cli": "^17.3.8",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/compiler-cli": "~17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^16.2.4",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@schematics/angular": "^16.2.2",
    "@types/jasmine": "~4.3.6",
    "@types/jasminewd2": "~2.0.11",
    "@types/node": "^14.18.54",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "angular-cli-ghpages": "^1.0.7",
    "core-js": "^3.32.2",
    "cpr": "^3.0.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "shx": "^0.3.4",
    "ts-node": "~10.9.1",
    "typescript": "5.4.5",
    "zone.js": "^0.14.10"
  },
  "dependencies": {
    "dayjs": "^1.11.4",
    "tslib": "^2.6.2"
  },
  "schematics": "./schematics/collection.json"
}
